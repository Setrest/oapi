{
    "name": "setrest/oapi",
    "description": "Auto generating api documentation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "email": "al.ho99@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=8.40.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Setrest\\OAPIDocumentation\\": "src/oapi"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Setrest\\OAPIDocumentation\\OpenAPIServiceProvider"
            ],
            "aliases": {
                "OAPIDocumentation": "Setrest\\OAPIDocumentation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}